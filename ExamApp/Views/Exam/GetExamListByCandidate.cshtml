@model List<ExamListModel>
@{
    ViewData["Title"] = "GetExamListByCandidate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table table-hover table-striped table-bordered">
    <tr>
        <th>Başlık</th>
        <th>Oluşturulma Tarihi</th>
        <th>İşlem</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.CreatedDate</td>
            <td>
                <a asp-action="GetAll" asp-controller="Exam" asp-route-id="@item.ExamId" class="btn btn-danger btn-sm"> Aç</a>
            </td>
        </tr>
    }
</table>